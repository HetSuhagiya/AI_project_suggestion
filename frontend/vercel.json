{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-url.com/api/$1"
    }
  ],
  "functions": {
    "app/api/**/*.py": {
      "runtime": "python3.9"
    }
  }
} 